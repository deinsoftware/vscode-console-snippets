{
  "name": "debug-snippets",
  "description": "VS Code Debug snippets for JS, TS and CSS",
  "version": "1.2.1",
  "displayName": "Debug Snippets",
  "publisher": "deinsoftware",
  "icon": "images/light-icon.png",
  "keywords": [
    "debugger",
    "console",
    "log",
    "snippets",
    "css",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deinsoftware/vscode-debug-snippets"
  },
  "sponsor": {
    "url": "https://ko-fi.com/equiman"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./build/extension.js",
  "browser": "./build/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onCommand:extension.snippetSearch"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/code.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/code.json"
      },
      {
        "language": "css",
        "path": "./snippets/styles.json"
      },
      {
        "language": "scss",
        "path": "./snippets/styles.json"
      }
    ]
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.15.0"
  }
}
